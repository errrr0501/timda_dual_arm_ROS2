        </geometry>
      </collision>
    </link>
    <joint
      name="${side}_tool_joint"
      type="revolute">
      <origin
        xyz="0 0.002 0.1045"
        rpy="-1.7985E-15 -2.9973E-16 1.1669E-16" />
      <parent
        link="${side}_tool_link" />
      <child
        link="${side}_tool_end_Link" />
      <axis
        xyz="0 1 0" />
      <limit
        lower="-3.14"
	    upper="3.14"
        effort="1000"
        velocity="6.5" />
      <dynamics damping="0.7"/>
    </joint>

    <link
